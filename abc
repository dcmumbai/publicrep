pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('build') {
            steps {
                echo 'Hello building'
            }
        }
        
        stage('deploy') {
            steps {
                echo 'Hello deploy World'
            }
        }
        stage('test') {
            steps {
                echo 'Hello test World'
            }
        }
        
    }
}
